{"ts":1380211148117,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"/* jshint expr: true */\n/* global describe, it, before, after */\n\nvar chai   = require(\"chai\"),\n    expect = chai.expect;\n    \nvar request = require(\"superagent\"),\n    conf    = require(\"../conf\"),\n    app     = require(\"../app\");\n    \nvar url = conf.IP + \":\" + conf.PORT + \"/\";\n    \ndescribe(\"App\", function() {\n    \n    before(function(done) {\n        app.listen(conf.PORT, done);\n    });\n    \n    after(function(done) {\n        app.close(done);\n    });\n\n    \n    it(\"should be on the web\", function(done) {\n        this.slow(500);\n        request.get(url, function(res) {\n            expect(res).to.exist;\n            expect(res.status).to.equal(200);\n            \n            done();\n        });\n    });\n    \n    it(\"should have a status of 404 when the pages doesn't exist\", function(done) {\n        this.slow(500);\n        request.get(url + \"does/not/exist\", function(res) {\n            expect(res).to.exist;\n            expect(res.status).to.equal(404);\n            \n            done();\n        });\n    });\n    \n    it(\"should have 'Hello World!' on the page\", function(done) {\n        this.slow(500);\n        request.get(url, function(res) {\n            expect(res).to.exist;\n            expect(res.status).to.equal(200);\n            expect(res.text).to.contain(\"Hello World!\");\n            \n            done();\n        });\n    });\n});"]],"start1":0,"start2":0,"length1":0,"length2":1343}]],"length":1343}
